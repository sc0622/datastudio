<?xml version='1.0' encoding='utf-8'?>
<jframe>
  <app seatEditable="true" system="仿真系统" name="simulator" userName="****">
    <config>
      <switch remember="true"/>
      <splitter opacity="0.2" width="4" color="#666" opaqueResize="true"/>
      <dock/>
      <busyIndicator duration="2000" model="true" image="" visible="true"/>
    </config>
    <system module="module #1" name="仿真系统">
      <seat-module>
        <item seat="seat #1" module="module #1"/>
      </seat-module>
      <module submodule="" name="module #1">
        <layout>
          <view>
            <horizontal collapsibile="0:true;1:true" scales="0.51:1.6" opaqueResize="false">
              <component id="com_simulator_nav" load="true"/>
              <component id="com_simulator" load="true"/>
            </horizontal>
          </view>
          <popup>
            <!--component load="true" id="xxx"/-->
          </popup>
        </layout>
      </module>
    </system>
    <!-- 以下为框架默认配置，请谨慎修改 -->
    <!-- list属性值：为[用户管理]、[系统管理]、[模块管理]、[权限管理]的组合 -->
    <system list="用户管理|系统管理|模块管理|权限管理" module="权限管理" name="管理员系统">
      <module submodule="" name="权限管理">
        <layout>
          <view>
            <horizontal scales="1:1">
              <component id="jcom_usermgr"/>
            </horizontal>
          </view>
        </layout>
      </module>
    </system>
  </app>
</jframe>
