<?xml version='1.0' encoding='utf-8'?>
<jframe>
  <app frameVersion="local" name="simulator" organizationName="Tengden" displayName="ICD System" applicationName="ICD System">
    <envval>
      <global>
        <path type="prefix" base="@FrameDir@/bin">
          <item path="3rdpart"/>
          <item path="3rdpart/Qt"/>
          <item path="3rdpart/ice"/>
          <item path="core"/>
        </path>
        <path type="append" base="@ThisDir@/bin">
          <item path="core"/>
          <item path="3rdpart/moxa"/>
        </path>
      </global>
    </envval>
    <qtconf desc="qt.conf content">
      <paths>
        <item value="@FrameDir@/bin/3rdpart/Qt/plugins" key="Plugins"/>
        <item value="@FrameDir@/bin/3rdpart/Qt/imports" key="Imports"/>
        <item value="@FrameDir@/bin/3rdpart/Qt/qml" key="Qml2Imports"/>
      </paths>
    </qtconf>
    <!-- type= -->
    <icddata name="file1">
      <file name="file1">
        <path>@ThisDir@/bin/data/Default.xml</path>
      </file>
      <database name="db1">
        <driver>SQL SERVER</driver>
        <serverName>DELL-0101\SQLSERVER2016</serverName>
        <database>ICDDB</database>
        <userName>ICDUser</userName>
        <password>ICDUser</password>
      </database>
    </icddata>
    <workMode id="1" name="Test1" dir="@ConfigDir@/workmode_1"/>
    <logging filePath="@ConfigDir@/jframe_logging.conf"/>
    <icdChannel filePath="@ThisDir@/config/icdsystem/icd_channels.conf"/>
    <system>
      <language path="@FrameDir@/config/language" name="Chinese (China)"/>
    </system>
    <textCodec encoding="gbk"/>
    <dbus serviceName="com.smartsoft.jframe1" enabled="false" path="/com/smartsoft/jframe/notify"/>
    <ice enabled="false">
      <server host="192.168.0.182" serviceName="com.smartsoft.jframe1" port="20000"/>
    </ice>
    <mainWindow windowTheme="Office2010Blue" windowIcon="@ConfigDir@/resource/app.ico" closeKilled="true" windowTitle="ICD系统" systemFont="微软雅黑,9,-1,5,50,0,0,0,0,0,Regula">
      <splash fullScreen="true" msgAlign="AlignHCenter|AlignBottom" imageFinish="@ConfigDir@/resource/finish.png" imageStart="@ThisDir@/config/icdsystem/resource/start.png" font="Microsoft Yahei, 24"/>
      <toolBar type="ribbonBar">
        <menuBar/>
        <toolButton/>
        <ribbonBar visible="true" minimized="false"/>
      </toolBar>
      <statusBar type=""/>
      <layout type="splitter"/>
    </mainWindow>
  </app>
</jframe>
