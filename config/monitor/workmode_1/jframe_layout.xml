<?xml version='1.0' encoding='utf-8'?>
<jframe>
  <app system="监控系统" userName="****" name="monitor" seatEditable="true">
    <config>
      <switch remember="true"/>
      <splitter opaqueResize="true" color="#666" opacity="0.2" width="4"/>
      <dock/>
      <busyIndicator image="" duration="2000" model="true" visible="true"/>
    </config>
    <system name="监控系统" module="system #1">
      <seat-module>
        <item seat="seat #1" module="module #1"/>
      </seat-module>
      <module submodule="" name="module #1">
        <layout>
          <view>
            <horizontal opaqueResize="false" scales="0.58:2.1" collapsibile="0:true;1:true">
              <vertical opaqueResize="false" scales="1.8:0.73" collapsibile="0:true;1:true">
                <component id="com_monitor_nav" load="true"/>
                <component id="com_utraledit" load="true"/>
              </vertical>
              <component id="com_monitor" load="true"/>
            </horizontal>
          </view>
        </layout>
      </module>
    </system>
    <!-- 以下为框架默认配置，请谨慎修改 -->
    <!-- list属性值：为[用户管理]、[系统管理]、[模块管理]、[权限管理]的组合 -->
    <system list="用户管理|系统管理|模块管理|权限管理" name="管理员系统" module="权限管理">
      <module submodule="" name="权限管理">
        <layout>
          <view>
            <horizontal scales="1:1">
              <component id="jcom_usermgr"/>
            </horizontal>
          </view>
        </layout>
      </module>
    </system>
  </app>
</jframe>
