<?xml version='1.0' encoding='utf-8'?>
<jframe>
  <app frameVersion="local" applicationName="ICD System" organizationName="Tengden" name="monitor" displayName="ICD System">
    <envval>
      <global>
        <path base="@FrameDir@/bin" type="prefix">
          <item path="3rdpart"/>
          <item path="3rdpart/Qt"/>
          <item path="3rdpart/ice"/>
          <item path="core"/>
        </path>
        <path base="@ThisDir@/bin" type="append">
          <item path="core"/>
          <item path="3rdpart/moxa"/>
        </path>
      </global>
    </envval>
    <qtconf desc="qt.conf content">
      <paths>
        <item key="Plugins" value="@FrameDir@/bin/3rdpart/Qt/plugins"/>
        <item key="Imports" value="@FrameDir@/bin/3rdpart/Qt/imports"/>
        <item key="Qml2Imports" value="@FrameDir@/bin/3rdpart/Qt/qml"/>
      </paths>
    </qtconf>
    <!-- type= -->
    <icddata name="file1">
      <file name="file1">
        <path>@ThisDir@/bin/data/Default.xml</path>
      </file>
      <database name="db1">
        <driver> SQL SERVER </driver>
        <serverName> DELL-0101\SQLSERVER2016 </serverName>
        <database> ICDDB </database>
        <userName> ICDUser </userName>
        <password> ICDUser </password>
      </database>
    </icddata>
    <workMode id="1" dir="@ConfigDir@/workmode_1" name="Test1"/>
    <logging filePath="@ConfigDir@/jframe_logging.conf"/>
    <icdChannel filePath="@ThisDir@/config/icdsystem/icd_channels.conf"/>
    <system>
      <language path="@FrameDir@/config/language" name="Chinese (China)"/>
    </system>
    <textCodec encoding="gbk"/>
    <dbus path="/com/smartsoft/jframe/notify" enabled="false" serviceName="com.smartsoft.jframe1"/>
    <ice enabled="false">
      <server port="20000" host="192.168.0.182" serviceName="com.smartsoft.jframe1"/>
    </ice>
    <mainWindow closeKilled="true" windowIcon="@ConfigDir@/resource/app.ico" windowTitle="ICD系统" systemFont="微软雅黑,9,-1,5,50,0,0,0,0,0,Regula" windowTheme="Office2010Blue">
      <splash fullScreen="true" font="Microsoft Yahei, 24" imageStart="@ThisDir@/config/icdsystem/resource/start.png" imageFinish="@ConfigDir@/resource/finish.png" msgAlign="AlignHCenter|AlignBottom"/>
      <toolBar type="ribbonBar">
        <menuBar/>
        <toolButton/>
        <ribbonBar visible="true" minimized="false"/>
      </toolBar>
      <statusBar type=""/>
      <layout type="splitter"/>
    </mainWindow>
  </app>
</jframe>
