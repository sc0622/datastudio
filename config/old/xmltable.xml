<jxmltable version="1.0">
  <table name="VehicleTable" orientations="horizontal" headerWidth="200" sync="false" >
    <!--机型信息表-->
    <item name="机型名称" type="string" editable="false" />
    <item name="机型编码" type="string" editable="false" />
    <item name="描述" type="string" editable="false" />
  </table>

  <table name="SystemTable" orientations="horizontal" headerWidth="200" sync="false" >
    <!--系统信息表-->
    <item name="系统名称" type="string" editable="false" />
    <item name="系统编码" type="string" editable="false" />
    <item name="描述" type="string" editable="false" />
  </table>

  <table name="Table" orientation="horizontal" headerWidth="200" sync="false">
    <!--表信息表-->
    <item name="名称" type="string" editable="false" />
    <item name="编码" type="string" editable="false" />
    <item name="长度" type="string" editable="false" />
    <item name="描述" type="string" editable="false" />
  </table>

  <table name="FrameTable" orientations="horizontal" headerWidth="180" sync="false">
    <!--帧定义表-->
    <item name="名称" type="string" editable="false" />
    <item name="帧码" type="string" editable="false" />
    <item name="时序" type="string" editable="false" />
    <item name="描述" type="string" editable="false" maxLength="60" />
  </table>

  <table name="ObjectTable" orientations="horizontal" headerWidth="180" sync="false">
    <!--通用属性表-->
    <item name="属性" type="string" editable="false" headerSize="120" />
    <item name="数值" type="string" editable="false" />
  </table>

  <table name="RuleTable" orientations="horizontal" headerWidth="180" sync="false">
    <!--规则概要信息表-->
    <item name="名称" type="string" editable="false" headerSize="180"/>
    <item name="编码" type="string" editable="false" headerSize="180"/>
    <item name="字节号" type="string" editable="false" headerSize="80"/>
    <item name="长度" type="string" editable="false" headerSize="80"/>
    <item name="数据类型" type="string" editable="false" headerSize="80"/>
    <item name="描述" type="string" editable="false" headerSize="200"/>
  </table>

  <table name="EigenvalueComTable" orientations="horizontal" headerWidth="180" sync="false">
    <!--特征值表-->
    <item name="数值" type="numeric" decimals="0" range="[-100000000,100000000]" editable="true" headerSize="70" />
    <item name="描述" type="string" editable="true" maxLength="40" />
  </table>
  
  <table name="EigenvalueBitTable" orientations="horizontal" headerWidth="180" sync="false">
    <!--bitValue表-->
    <item name="数值" type="string" editable="false" headerSize="50" />
    <item name="描述" type="string" editable="true" maxLength="60" />
    <item name="二进制" type="string" editable="false" headerSize="70" />
  </table>
  
  <table name="EigenvalueMapTable" orientations="horizontal" headerWidth="180" sync="false">
    <!--bitMap表-->
    <item name="比特位" type="string" editable="false" headerSize="50" />
    <item name="描述" type="string" editable="true" maxLength="60" headerSize="170"/>
    <item name="0值含义" type="string" editable="true" headerSize="75" />
    <item name="1值含义" type="string" editable="true" headerSize="75" />
  </table>
</jxmltable>
