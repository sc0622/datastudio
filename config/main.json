{
   "global" : {
      "channel" : {
         "filePath" : "@ConfigDir@/channel.conf"
      },
      "chart" : {
         "autoSave" : true,
         "filePath" : "chart.json"
      },
      "language" : "Chinese (China)",
      "menubar" : {
         "toolBarVisible" : true
      },
      "theme" : {
         "name" : "dark"
      }
   },
   "module" : {
      "analyse" : {
         "option" : {
            "chart" : {
               "showYAlign" : true,
               "showYLabel" : true,
               "syncTrack" : true,
               "xAxisSync" : true
            },
            "tree" : {
               "loadDeep" : 1,
               "showOffset" : true,
               "showType" : true
            }
         },
         "parser" : {
            "filePath" : "@ConfigDir@/protocol.json",
            "sourceType" : "file"
         }
      },
      "edit" : {
         "option" : {
            "chart" : {},
            "tree" : {
               "loadDeep" : 2
            }
         },
         "parser" : {
            "filePath" : "@ConfigDir@/protocol.json",
            "sourceType" : "file"
         }
      },
      "monitor" : {
         "option" : {
            "chart" : {
               "columnCount" : 1,
               "flushEnabled" : true,
               "showYAlign" : true,
               "showYLabel" : true,
               "syncTrack" : true,
               "xAxisSync" : true,
               "yLabelWidth" : 10
            },
            "tree" : {
               "flushEnabled" : true,
               "loadDeep" : 1,
               "showDesc" : true,
               "showOffset" : true,
               "showOrignal" : 10,
               "showType" : true,
               "showValue" : true
            }
         },
         "parser" : {
            "filePath" : "@ConfigDir@/protocol.json",
            "sourceType" : "file"
         }
      },
      "simulate" : {
         "option" : {
            "tree" : {
               "flushEnabled" : true,
               "loadDeep" : 1,
               "showDesc" : true,
               "showOffset" : true,
               "showOrignal" : 10,
               "showType" : true,
               "showValue" : true
            }
         },
         "parser" : {
            "filePath" : "@ConfigDir@/protocol.json",
            "sourceType" : "file"
         }
      }
   }
}
