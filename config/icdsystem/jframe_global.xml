<?xml version='1.0' encoding='utf-8'?>
<jframe>
  <app frameVersion="local" organizationName="Tengden" name="icdsystem" applicationName="ICD System" displayName="ICD System">
    <envval>
      <global>
        <path base="@FrameDir@/bin" type="prefix">
          <item path="3rdpart"/>
          <item path="3rdpart/Qt"/>
          <item path="3rdpart/ice"/>
          <item path="core"/>
        </path>
        <path base="@ThisDir@/bin" type="append">
          <item path="core"/>
          <item path="3rdpart/moxa"/>
        </path>
      </global>
    </envval>
    <qtconf desc="qt.conf content">
      <paths>
        <item key="Plugins" value="@FrameDir@/bin/3rdpart/Qt/plugins"/>
        <item key="Imports" value="@FrameDir@/bin/3rdpart/Qt/imports"/>
        <item key="Qml2Imports" value="@FrameDir@/bin/3rdpart/Qt/qml"/>
      </paths>
    </qtconf>
    <!-- type= -->
    <icddata name="file1">
      <file name="file1">
        <path>@AppDir@/data/Default.xml</path>
      </file>
      <database name="db1">
        <driver>SQL SERVER</driver>
        <serverName>DELL-0101\SQLSERVER2016</serverName>
        <database>ICDDB</database>
        <userName>ICDUser</userName>
        <password>ICDUser</password>
        <tableLimitation>100</tableLimitation>
      </database>
    </icddata>
    <workMode id="1" name="Test1" dir="@ConfigDir@/workmode_1"/>
    <logging filePath="@ConfigDir@/jframe_logging.conf"/>
    <icdChannel filePath="@ThisDir@/config/icdsystem/icd_channels.conf"/>
    <system>
      <language path="@FrameDir@/config/language" name="Chinese (China)"/>
    </system>
    <textCodec encoding="gbk"/>
    <dbus path="/com/smartsoft/jframe/notify" serviceName="com.smartsoft.jframe1" enabled="false"/>
    <ice enabled="false">
      <server port="20000" serviceName="com.smartsoft.jframe1" host="192.168.0.182"/>
    </ice>
    <mainWindow systemFont="微软雅黑,9,-1,5,50,0,0,0,0,0,Regular" windowIcon="@ConfigDir@/resource/app.ico" windowTitle="ICD系统" windowTheme="Office2010Blue" closeKilled="true">
      <splash msgAlign="AlignHCenter|AlignBottom" font="Microsoft Yahei,24" msgColor="#51dd52" imageStart="@ConfigDir@/resource/start.jpg" imageStartSize="1024, 768" imageFinish="@ConfigDir@/resource/finish.png" fullScreen="false"/>
      <toolBar type="ribbonBar">
        <menuBar/>
        <toolButton/>
        <ribbonBar minimized="false" visible="true"/>
      </toolBar>
      <statusBar type=""/>
      <layout type="splitter"/>
    </mainWindow>
  </app>
</jframe>
