<?xml version='1.0' encoding='utf-8'?>
<jframe>
  <app displayName="ICD System" frameVersion="local" applicationName="ICD System" organizationName="Tengden" name="analyse">
    <envval>
      <global>
        <path base="@FrameDir@/bin" type="prefix">
          <item path="3rdpart"/>
          <item path="3rdpart/Qt"/>
          <item path="3rdpart/ice"/>
          <item path="core"/>
        </path>
        <path base="@ThisDir@/bin" type="append">
          <item path="core"/>
          <item path="3rdpart/moxa"/>
        </path>
      </global>
    </envval>
    <qtconf desc="qt.conf content">
      <paths>
        <item key="Plugins" value="@FrameDir@/bin/3rdpart/Qt/plugins"/>
        <item key="Imports" value="@FrameDir@/bin/3rdpart/Qt/imports"/>
        <item key="Qml2Imports" value="@FrameDir@/bin/3rdpart/Qt/qml"/>
      </paths>
    </qtconf>
    <!-- type= -->
    <icddata name="file1">
      <file name="file1">
        <path>@ThisDir@/bin/data/Default.xml</path>
      </file>
      <database name="db1">
        <driver>SQL SERVER</driver>
        <serverName>DELL-0101\SQLSERVER2016</serverName>
        <database>ICDDB</database>
        <userName>ICDUser</userName>
        <password>ICDUser</password>
      </database>
    </icddata>
    <workMode id="1" name="Test1" dir="@ConfigDir@/workmode_1"/>
    <logging filePath="@ConfigDir@/jframe_logging.conf"/>
    <icdChannel filePath="@ThisDir@/config/icdsystem/icd_channels.conf"/>
    <system>
      <language name="Chinese (China)" path="@FrameDir@/config/language"/>
    </system>
    <textCodec encoding="gbk"/>
    <dbus serviceName="com.smartsoft.jframe1" enabled="false" path="/com/smartsoft/jframe/notify"/>
    <ice enabled="false">
      <server serviceName="com.smartsoft.jframe1" port="20000" host="192.168.0.182"/>
    </ice>
    <mainWindow windowTheme="Office2010Blue" closeKilled="true" windowTitle="ICD数据分析系统" systemFont="微软雅黑,9,-1,5,50,0,0,0,0,0,Regula" windowIcon="@ConfigDir@/resource/app.ico">
      <splash imageStart="@ThisDir@/config/icdsystem/resource/start.png" font="Microsoft Yahei, 24" imageFinish="@ConfigDir@/resource/finish.png" fullScreen="true" msgAlign="AlignHCenter|AlignBottom"/>
      <toolBar type="ribbonBar">
        <menuBar/>
        <toolButton/>
        <ribbonBar minimized="false" visible="true"/>
      </toolBar>
      <statusBar type=""/>
      <layout type="splitter"/>
    </mainWindow>
  </app>
</jframe>
