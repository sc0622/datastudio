
/** QWidget **/

QDialog {
    /*background: #293956;*/
}

/** * **/

QMessageBox QPushButton,
QDialogButtonBox QPushButton {
    min-width: 6em;
}

/** QMessageBox **/

QMessageBox {
    background: white;
    color: #ccc;
}

/** QMainWindow **/

QMainWindow {
    background: #293956;
}

QMainWindow::separator {
    background: #293a56;
    width: 6px;
    height: 6px;
}

QMainWindow::separator:hover {
    background: #666;
}
  
/** QTabWidget **/

QTabWidget {
    background: #1e1e1e;
}

QTabWidget::pane {
    border: 0px solid #3a3a3a;
    position: absolute;
    top: 0.0em;
    /*background: #394966;*/
}

QTabWidget::tab-bar {
    alignment: left;
}

QTabBar::tab {
    background: #364e6f;
    min-width: 8ex;
    padding: 6px;
    padding-left: 12px;
    padding-right: 12px;
    color: white;
}

QTabWidget QTabBar::tab:disabled {
    width: 0;
    background: transparent
}

QTabWidget QTabBar QToolButton {
    background: #e0777799;
    border: 1px solid #899;
}

QTabWidget QTabBar QToolButton:hover {
    background: #e077f799;
}

QTabWidget QTabBar QToolButton:pressed {
    background: #444466;
}

QTabWidget QTabBar QToolButton:disabled {
    background: #e0aaaaaa;
}

/** QTabBar **/

QTabBar {
    /*background: #293956;*/
}

QTabBar::tab:selected {
    background: #fff29d;
    color: black;
} 

QTabBar::tab:hover {
    background: #ffff2d;
    color: black;
}

/** QDockWidget **/

QDockWidget {
    background: #1e1e1e;
    color: white;
}

QDockWidget::title {
    background: #4d6082;
    color: #b0e9ed;
    text-align: left;
    padding-left: 3px;
    padding-top: 4px;
    padding-bottom: 4px;
}

QDockWidget::close-button, QDockWidget::float-button {
    background: #ced4dd;
    padding: 0px;
    icon-size: 20px;
}

QDockWidget::close-button:hover, QDockWidget::float-button:hover {
    background: darkgray;
    border: 0px;
}

QDockWidget::close-button:pressed, QDockWidget::float-button:pressed {
    padding: 1px -1px -1px 1px;
}

QDockWidget::close-button {
    subcontrol-position: center right;
    subcontrol-origin: margin;
    position: absolute;
    right: 5px;
    width: 26px;
    min-height: 21px;
}

QDockWidget::float-button {
    subcontrol-position: center right;
    subcontrol-origin: margin;
    position: absolute;
    right: 33px;
    width: 26px;
    min-height: 21px;
}

QDockWidget QTabBar::tab {
    padding: 8px;
    padding-left: 6px;
    padding-right: 6px;
}

/** QMenuBar **/

QMenuBar {
    background: #d6dbe9;
    color: black;
}

QMenuBar::item {
    padding: 4px 6px;
    border-radius: 1px;
}

QMenuBar::item:hover {
    background: #fdf4bf;
    border: 1px solid #e5c365;
}

QMenuBar::item:selected {
    background: #fdf4bf;
    border: 1px solid #e5c365;
}

/** QMenu **/

QMenu {
    background: #eaf0ff;
    color: black;
    border: 1px solid #9ba7b7;
}

QMenu::item {
    padding: 4px 30px 4px 25px;
    border: 1px solid transparent;
}

QMenu::item:selected {
    background: #fdf4bf;
}

QMenu::item:disabled {
    background: #eaf0ff;
    color: #777;
}

QMenu::icon {
    left: 3px;
}

QMenu::icon:checked {
    background: gray;
    border: 1px inset gray;
    position: absolute;
    top: 1px;
    right: 1px;
    bottom: 1px;
    left: 1px;
}

QMenu::separator {
    height: 1px;
    background: #bec3cb;
}

QMenu::indicator {
    width: 13px;
    height: 18px;
}
