QToolButton:checked {
    background: #fff29d;
}

QToolButton[popupMode="1"] {
    padding-right: 12px;
}

QToolButton::menu-button {
    padding-right: 3px;
    width: 10px;
} 

QToolButton::menu-button:!hover {
    background: transparent;
}

QToolButton::menu-arrow:hover {
      top: 2px; 
      left: 2px;
}

/** QToolBar **/

QToolBar {
    background: #cfd6e5;
    min-height: 30px;
}

QToolBar::separator {
    width: 1px;
    background: #8591a2;
}

QToolBar QToolButton {
    background: transparent;
    padding: 1px;
    border: none;
}

QToolBar QToolButton:hover {
    background: #fdf4bf;
}

QToolBar QToolButton:pressed {
    background: #fff29d;
}

/* QGroupBox */

QGroupBox {
    background: #d6dbe9;
    border-radius: 2px;
    border: 1px solid #d6dbe9;
    margin-top: 0.8em;
    padding-top: 0.5em;
    color: black;
    font-weight: bold;
} 

QGroupBox::title {
    background: #d6dbe9;
    color: black;
    border-top-left-radius: 2px;
    border-top-right-radius: 2px;
    border: 0px solid transparent;
    border-top: 1px solid #d6dbe9;
    border-left: 1px solid #d6dbe9;
    border-right: 1px solid #d6dbe9;
    subcontrol-origin: margin;
    subcontrol-position: top left;
    left: 0.0em;
    top: 0;
    padding-left: 30px;
    padding-right: 30px;
    padding-top: 2px;
    padding-bottom: 2px;
}

/** QLineEdit **/

QLineEdit {
    border-radius: 2px;
    padding: 2px;
    border: 1px solid #bbb;
    color: black;
    outline: none;
}

QLineEdit:read-only {
    background: #eee;
    color: #222;
}

QLineEdit:hover {
    border: 1px solid darkblue;
    color: darkblue;
} 

QLineEdit:focus:!read-only {
    color: darkblue;
    border: 1px solid darkblue;
    border-radius: 2px;
}

/** QComboBox, QSpinBox, QDoubleSpinBox **/

QComboBox,QSpinBox,QDoubleSpinBox,
QTimeEdit,QDateEdit,QDateTimeEdit {
    border-radius: 2px;
    border: 1px solid #bbb;
    padding: 2px;
    color: black;
    outline: none;
}

QComboBox:disabled,QSpinBox:disabled,QDoubleSpinBox:disabled,
QTimeEdit:disabled,QDateEdit:disabled,QDateTimeEdit:disabled {
    background: #eee;
}

QComboBox:editable {
    background: #fff;
}

QSpinBox:!editable, QDoubleSpinBox:!editable, QComboBox:!editable,
QComboBox::drop-down:!editable {

}

QComboBox:disabled,QSpinBox:disabled,QDoubleSpinBox:disabled,
QTimeEdit:disabled,QDateEdit:disabled,QDateTimeEdit:disabled {
    color: #333;
}

QComboBox:hover,QSpinBox:hover,QDoubleSpinBox:hover,
QTimeEdit:hover,QDateEdit:hover,QDateTimeEdit:hover {
    border: 1px solid #205375;
}

QComboBox:focus:!read-only,QSpinBox:focus:!read-only,QDoubleSpinBox:focus:!read-only,
QTimeEdit:focus:!read-only,QDateEdit:focus:!read-only,QDateTimeEdit:focus:!read-only {
    
}

QComboBox::down-arrow {
    image: url(:/datastudio/image/system/arrow_down.png);
}

QComboBox::drop-down,QSpinBox::drop-down,QDoubleSpinBox::drop-down,
QTimeEdit::drop-down,QDateEdit::drop-down,QDateTimeEdit::drop-down {
    width: 16px;
    background: #eee;
    subcontrol-origin: padding;
    subcontrol-position: top right;
    border-left-width: 1px;
    border-left-color: #bbb;
    border-left-style: solid;
    border-top-right-radius: 2px;
    border-bottom-right-radius: 2px;
}

QComboBox::drop-down:hover,QSpinBox::drop-down:hover,QDoubleSpinBox::drop-down:hover,
QTimeEdit::drop-down:hover,QDateEdit::drop-down:hover,QDateTimeEdit::drop-down:hover {
    background: #ddd;
}

QComboBox QAbstractItemView {
    background: #eaf0ff;
    border: 1px solid #445;
    selection-background-color: #fff29d;
    selection-color: black;
    outline: none;
}

QSpinBox::up-button,QDoubleSpinBox::up-button,
QTimeEdit::up-button,QDateEdit::up-button,QDateTimeEdit::up-button {
    margin: 1px 1px;
    background: #fff;
    subcontrol-origin: border;
    subcontrol-position: top right;
    width: 16px;
    border: 1px solid #aaa;
}

QSpinBox::up-button:hover,QDoubleSpinBox::up-button:hover,
QTimeEdit::up-button:hover,QDateEdit::up-button:hover,
QDateTimeEdit::up-button:hover {
    background: #eee;
    border: 1px solid #aaa;
}

QSpinBox::up-button:pressed,QDoubleSpinBox::up-button:pressed,
QTimeEdit::up-button:pressed,QDateEdit::up-button:pressed,
QDateTimeEdit::up-button:pressed {
    background: #ccc;
}

QSpinBox::up-arrow,QDoubleSpinBox::up-arrow,
QTimeEdit::up-arrow,QDateEdit::up-arrow,QDateTimeEdit::up-arrow {
    image: url(:/datastudio/image/system/up_arrow.png);
    width: 7px;
    height: 7px;
}

QSpinBox::up-arrow:disabled,QDoubleSpinBox::up-arrow:disabled,
QTimeEdit::up-arrow:disabled,QDateEdit::up-arrow:disabled,
QDateTimeEdit::up-arrow:disabled {
    image: url(:/datastudio/image/system/up_arrow_disabled.png);
}

QSpinBox::down-button,QDoubleSpinBox::down-button,
QTimeEdit::down-button,QDateEdit::down-button,QDateTimeEdit::down-button {
    margin: 0 1px 1px;
    background: #fff;
    subcontrol-origin: border;
    subcontrol-position: bottom right;
    width: 16px;
    border: 1px solid #aaa;
}

QSpinBox::down-button:hover,QDoubleSpinBox::down-button:hover,
QTimeEdit::down-button:hover,QDateEdit::down-button:hover,
QDateTimeEdit::down-button:hover {
    background: #eee;
    border: 1px solid #aaa;
}

QSpinBox::down-button:pressed,QDoubleSpinBox::down-button:pressed,
QTimeEdit::down-button:pressed,QDateEdit::down-button:pressed,
QDateTimeEdit::down-button:pressed {
    background: #ccc;
}

QSpinBox::down-arrow,QDoubleSpinBox::down-arrow,
QTimeEdit::down-arrow,QDateEdit::down-arrow,QDateTimeEdit::down-arrow {
    image: url(:/datastudio/image/system/down_arrow.png);
    margin-top: 2px;
    width: 7px;
    height: 7px;
}

QSpinBox::down-arrow:disabled,QDoubleSpinBox::down-arrow:disabled,
QTimeEdit::down-arrow:disabled,QDateEdit::down-arrow:disabled,
QDateTimeEdit::down-arrow:disabled {
    image: url(:/datastudio/image/system/down_arrow_disabled.png);
}

/** QListView **/

QListView {
    background: #262626;
    show-decoration-selected: 1;
}

QListView::item:alternate {
    background: #eee;
}

QListView::item:selected {
    border: 1px solid #6a6ea9;
}

QListView::item:selected:!active {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #ABAFE5, stop: 1 #8588B2);
}

QListView::item:selected:active {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #6a6ea9, stop: 1 #888dd9);
}

QListView::item:hover {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #FAFBFE, stop: 1 #DCDEF1);
}

/** QCheckBox **/

QCheckBox {
    color: black;
    outline: none;
}

QCheckBox::indicator:unchecked {
    image: url(:/datastudio/image/system/chkbox-uchkd-normal.png);
}

QCheckBox::indicator:unchecked:hover {
    image: url(:/datastudio/image/system/chkbox-uchkd-hover.png);
}

QCheckBox::indicator:unchecked:pressed {
    image: url(:/datastudio/image/system/chkbox-uchkd-pressed.png);
}

QCheckBox::indicator:unchecked:disabled {
    image: url(:/datastudio/image/system/chkbox-uchkd-disabled.png);
}

QCheckBox::indicator:indeterminate {
    image: url(:/datastudio/image/system/chkbox-part-normal.png);
}

QCheckBox::indicator:indeterminate:hover {
    image: url(:/datastudio/image/system/chkbox-part-hover.png);
}

QCheckBox::indicator:indeterminate:pressed {
    image: url(:/datastudio/image/system/chkbox-part-pressed.png);
}

QCheckBox::indicator:indeterminate:disabled {
    image: url(:/datastudio/image/system/chkbox-part-disabled.png);
}

QCheckBox::indicator:checked {
    image: url(:/datastudio/image/system/chkbox-chkd-normal.png);
}

QCheckBox::indicator:checked:hover {
    image: url(:/datastudio/image/system/chkbox-chkd-hover.png);
}

QCheckBox::indicator:checked:pressed {
    image: url(:/datastudio/image/system/chkbox-chkd-pressed.png);
}

QCheckBox::indicator:checked:disabled {
    image: url(:/datastudio/image/system/chkbox-chkd-disabled.png);
}

/** QRadioButton **/

QRadioButton {
    outline: none;
}

QRadioButton::indicator {
    width: 15px;
    height: 15px;
    outline: none;
}

QRadioButton::indicator:unchecked {
    image: url(:/datastudio/image/system/radiobtn-uchkd-normal.png);
}

QRadioButton::indicator:unchecked:hover {
    image: url(:/datastudio/image/system/radiobtn-uchkd-hover.png);
}

QRadioButton::indicator:unchecked:pressed {
    image: url(:/datastudio/image/system/radiobtn-uchkd-pressed.png);
}

QRadioButton::indicator:unchecked:disabled {
    image: url(:/datastudio/image/system/radiobtn-uchkd-disabled.png);
}

QRadioButton::indicator:checked {
    image: url(:/datastudio/image/system/radiobtn-chkd-normal.png);
}

QRadioButton::indicator:checked:hover {
    image: url(:/datastudio/image/system/radiobtn-chkd-hover.png);
}

QRadioButton::indicator:checked:disabled {
    image: url(:/datastudio/image/system/radiobtn-chkd-disabled.png);
}

/** QTextEdit **/

QTextEdit {
    border: 1px solid #334;
    selection-color: white;
    selection-background-color: darkblue;
    font-family: "Consolas";
    font-weight: bold;
}

QTextEdit:focus {
    border: 1px solid #446;
}

/** QScrollBar **/

QScrollBar {
    background: #e8e8ec;
    border-radius: 0;
}

QScrollBar:horizontal {
    height: 12px;
    margin: 0 12px 0 12px;
    border-top: 1px solid #ccc;
    border-bottom: 1px solid #ccc;
}

QScrollBar::handle:horizontal {
    background: #a0a1a7;
    min-width: 4px;
    margin: 1px;
    border-radius: 3px;
}

QScrollBar:handle:horizontal:hover {
    background: #808187;
}

QScrollBar::add-line:horizontal {
    background: #a0a1a7;
    width: 12px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal {
    background: #a0a1a7;
    width: 12px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar::up-arrow:horizontal,
QScrollBar::down-arrow:horizontal {
    border: 0;
    width: 0;
    height: 0;
    background: none;
}

QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal {
    background: none;
}

QScrollBar:vertical {
    width: 12px;
    margin: 12px 0 12px 0;
    border-left: 1px solid #ccc;
    border-right: 1px solid #ccc;
}

QScrollBar::handle:vertical {
    background: #a0a1a7;
    min-height: 4px;
    margin: 1px;
    border-radius: 3px;
}

QScrollBar:handle:vertical:hover {
    background: #808187;
}

QScrollBar::add-line:vertical {
    background: #a0a1a7;
    height: 12px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical {
    background: #a0a1a7;
    height: 12px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

QScrollBar::up-arrow:vertical,
QScrollBar::down-arrow:vertical {
    border: 0;
    width: 0;
    height: 0;
    background: none;
}

QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical {
    background: none;
}

