/** QToolBar **/

QWidget#JChartView QToolBar {
    background: #323232;
    spacing: 2px;
    min-height: 20px;
    padding: 0;
} 

QWidget#JChartView QToolBar::separator {
    width: 1px;
    background: #555;
}
 
QWidget#JChartView QToolBar QToolButton {
    background: transparent;
    border: 0;
}

QWidget#JChartView QToolBar QToolButton:hover {
    background: #445;
    border: 0;
}

QWidget#JChartView QToolBar QToolButton:pressed {
    background: #446;
    border: 0;
}

/** QToolBox **/

QToolBox {
    background: #aaa;
    border: 1px solid #666;
}
QToolBox::tab {
    background: #cce;
    color: #222;
}

QToolBox::tab:hover {
    background: #acf;
}

QToolBox::tab:selected {
    background: #fff29d;
    font: italic bold 14px;
}

/** QTreeView QLineEdit **/

QTreeView QLineEdit {
    border-radius: 0;
    padding: 0;
    border: 0;
}

QTreeView  QLineEdit:hover {
    border: 0;
} 

QTreeView QLineEdit:focus:!read-only {
    border-radius: 0;
}

/**********************************************************/

/** QToolButton#buttonClose **/

QToolButton#buttonClose {
    background: #05383838;
    padding: 1px;
    border-radius: 1px;
    border: 1px solid transparent;
}

QToolButton#buttonClose::separator {
    width: 1px;
    background: #a0555555;
}

QToolButton#buttonClose:hover {
    background: #30444455;
    border: 0;
}

QToolButton#buttonClose:pressed {
    background: #50444466;
    border: 0;
}

QScrollArea, GeneralWidget {
    background: white; 
    outline: none;
}
