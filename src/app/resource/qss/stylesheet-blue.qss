/** QToolTip **/

QToolTip {
    background: #eaf0ff;
    border: 1px solid #9ba7b7;
    color: black;
    border: 1px solid #555;
}

/** QHeaderView **/

QHeaderView {
    background: transparent;
    border: 0;
    outline: none;
}

QHeaderView::section {
    background: #ddd;
    outline: none;
}

QHeaderView::section:horizontal {
    border-left: 1px solid #bbb;
    border-right: 0;
    border-top: 0;
    border-bottom: 0;
    padding-left: 5px;
    padding-top: 4px;
    padding-bottom: 4px;
}

QHeaderView::section:vertical {
    border-left: 0;
    border-right: 0;
    border-top: 0; 
    border-bottom: 1px solid #ccc;
    padding-left: 4px;
    padding-right: 4px;
}

QHeaderView::section:hover {
    background: #a0fff29d;
}

QHeaderView::section:checked {
    background: #3399ff;
    color: #ffa;
    font-style: italic;
}

/** QTreeView **/

QTreeView {
    background: white;
    border: 1px solid transparent;
    alternate-background-color: #28283f;
    selection-background-color: #282828;
    gridline-color: #2f2f2f;
    outline: none;
}

QTreeView::item {
    background: #e6ebf9;
    color: black;
    border-left: 1px solid white;
    border-bottom: 1px solid white;
    padding-left: 2px;
    padding-right: 2px;
    height: 1.3em;
    outline: none;
}

QTreeView::item:hover {
    background: #ffe24d;
}

QTreeView::item:focus {
    outline: none;
}

QTreeView::item:selected,
QTreeView::item:selected:!focus {
    background: #fff29d;
    font-weight: bold;
}

QTreeView::branch:has-siblings:!adjoins-item {
    border-image: url(:/datastudio/image/system/dark/vline.png) 0;
}

QTreeView::branch:has-siblings:adjoins-item {
    border-image: url(:/datastudio/image/system/dark/branch-more.png) 0;
}

QTreeView::branch:!has-siblings:adjoins-item {
    border-image: url(:/datastudio/image/system/dark/branch-end.png) 0;
}

QTreeView::branch:has-children:!has-siblings:closed,
QTreeView::branch:closed:has-children:has-siblings {
    image: url(:/datastudio/image/system/dark/branch-closed.png);
}

QTreeView::branch:open:has-children:!has-siblings,
QTreeView::branch:open:has-children:has-siblings  {
    image: url(:/datastudio/image/system/dark/branch-open.png);
}

/** QTableView **/

QTableView {
    background: white;
    gridline-color: #aaa;
    outline: none;
}

QTableView::item { 
    background: #eee;
    padding: 0px;
    outline: none;
}

QTableView::item:hover {
    background: #a0fff29d;
}

QTableView::item:focus {
    border: 0px solid #fff29d;
}

QTableView::item:selected,
QTableView::item:selected:!focus {
    background: #3399ff;
    color: #fff;
}

QTableView QTableCornerButton::section {
    color: black;
    background: transparent;
}

/** QStatusBar **/

QStatusBar {
    background: #007acc;
    min-height: 25px;
}

/** QPushButton **/

QPushButton { 
    border-radius: 2px; 
    border: 1px solid #aaaf30;
    background: #c0cccc; 
    color: black;
    padding-left: 0.6em;
    padding-right: 0.6em;
    padding-top: 0.2em;
    padding-bottom: 0.2em;
    outline: none;
}

QPushButton:hover {
    background: #50cccc;
}
 
QPushButton:pressed {
    background: #5d5c91;
    color: white;
}

QPushButton:disabled {
    background: #fff;
    color: #888;
    border: 1px solid #aaafa0;
}

