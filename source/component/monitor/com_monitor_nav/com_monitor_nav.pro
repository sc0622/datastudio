this_dir = $$PWD/../../../..

QT += gui

greaterThan(QT_MAJOR_VERSION, 4): QT += widgets

DEFINES += \
    PRO_COMPONENT \
    ICDWIDGET_LIB

include($$this_dir/source/common/build.pri)

TARGET = $$qtLibraryTarget(com_monitor_nav)

# The .h file which was generated for your project. Feel free to hack it.
HEADERS += \
    $$PWD/commonitornav.h \
    $$PWD/commonitornavui.h \
    mainnav_widget.h

# The .cpp file which was generated for your project. Feel free to hack it.
SOURCES += \
    $$PWD/commonitornav.cpp \
    $$PWD/commonitornavui.cpp \
    mainnav_widget.cpp

#
exists($$PWD/com_monitor_nav.xml) {
    OTHER_FILES += \
        $$PWD/com_monitor_nav.xml
}

#-------------------------------------------------
# import libraries
#-------------------------------------------------

#-------------------------------------------------
# global commands
#-------------------------------------------------

win32|unix: {
    commands += echo --- console - com_monitor_nav --- &

    ## copy files
    dstdir = $$this_dir/component/$$systemdir/com_monitor_nav/
    win32:dstdir = $$replace(dstdir, /, \\)
    !exists("$$dstdir"):commands += $(MKDIR) "\"$$dstdir\"" &
    srcfile = $$PWD/com_monitor_nav.xml
    win32:srcfile = $$replace(srcfile, /, \\)
    exists("$$srcfile"):commands += $(COPY_FILE) "\"$$srcfile\"" "\"$$dstdir\"" &

    QMAKE_POST_LINK += $$commands
}
