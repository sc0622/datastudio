
/** QWidget **/

QDialog {
    background: #353535;
}

QWidget {
    color: #fff;
}

/** * **/

QMessageBox QPushButton,
QDialogButtonBox QPushButton {
    min-width: 4em; 
}

/** QMessageBox **/

QMessageBox {
    background: #353535;
    color: #ccc;
}

/** QMainWindow **/

QMainWindow {
    background: #3a3a3a;
}

/** QTabWidget **/

QTabWidget {
    background: #1e1e1e;
}

QTabWidget::pane {
    border: 0px solid #3a3a3a;
    border-radius: 2px;
    position: absolute;
    top: 0.0em;
    background: #262626;
}

QTabWidget::tab-bar {
    alignment: left;
}

QTabBar::tab {
    background: #2a2a2f;
    min-width: 8ex;
    padding: 6px;
    padding-left: 12px;
    padding-right: 12px;
    color: #999;
}

/** QTabBar **/

QTabBar {
    background: transparent;
}

QTabBar::tab:selected {
    background: #262626;
    color: #fff;
}

QTabBar::tab:hover {
    background: #202020;
    color: #eee;
}

/** QDockWidget **/

QDockWidget {
    background: #1e1e1e;
}

QDockWidget::title {
     color: #ccc;
     text-align: left;
     background: #2d2d2d;
     padding-left: 3px;
     padding-top: 4px;
     padding-bottom: 4px;
}

QDockWidget::close-button, QDockWidget::float-button {
    background: #55555a;
    padding: 0px;
    icon-size: 20px;
}

QDockWidget::close-button:hover, QDockWidget::float-button:hover {
    background: darkgray;
    border: 0px;
}

QDockWidget::close-button:pressed, QDockWidget::float-button:pressed {
    padding: 1px -1px -1px 1px;
}

QDockWidget::close-button {
    subcontrol-position: center right;
    subcontrol-origin: margin;
    position: absolute;
    right: 2px;
    width: 25px;
    min-height: 21px;
}

QDockWidget::float-button {
    subcontrol-position: center right;
    subcontrol-origin: margin;
    position: absolute;
    right: 28px;
    width: 25px;
    min-height: 21px;
}

QDockWidget QTabBar::tab {
    padding: 8px;
    padding-left: 6px;
    padding-right: 6px;
}

/** QMenuBar **/

QMenuBar {
    background: #252526;
}

QMenuBar::item {
    padding: 4px 6px;
    border-radius: 1px;
}

QMenuBar::item:selected {
    background: #555;
}

/** QMenu **/

QMenu {
    background: #202020;
    margin: 1px;
}

QMenu::item {
    padding: 4px 30px 4px 22px;
    border: 1px solid transparent;
}

QMenu::item:selected {
    background: #444;
}

QMenu::icon:checked {
    background: gray;
    border: 1px inset gray;
    position: absolute;
    top: 1px;
    right: 1px;
    bottom: 1px;
    left: 1px;
}

QMenu::separator {
    height: 2px;
    background: #555;
    margin-left: 10px;
    margin-right: 5px;
}

QMenu::indicator {
    width: 13px;
    height: 18px;
}
